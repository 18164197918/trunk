export default {
  info: {
    cssObj   : {
      self : {
        css: {
          width : '50px',
          height: '50px',
        },
      },
      inner: {
        css     : {
          color          : 'rgba(255,255,255,1)',
          backgroundColor: 'rgba(69,76,101,1)',
          borderRadius   : '8px',
          transition     : 'all 0.2s 0s ease',
          backgroundImage: 'linear-gradient(0deg, rgba(100,110,136,1), rgba(100,110,136,1))',
          border         : '1px solid rgba(53,60,88,1)',
          boxShadow      : 'inset -23px 54px 27px -47px rgba(100,110,136,1),0px 2px 3px 0px rgba(0,0,0,.5)',

        },
        hoverCss: {
          transform: 'translate3d(0px,2px,0px)',
        },
      },
    },
    tplCssObj: {
      self: {
        class: '',
        css  : {
          width : '50px',
          height: '50px',
        },
      },
    },
  },
}

